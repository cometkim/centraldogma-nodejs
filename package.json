{
    "name": "@matsumana/centraldogma-nodejs",
    "version": "0.6.0",
    "description": "Central Dogma client for Node.js",
    "author": "Manabu Matsuzaki",
    "license": "Apache-2.0",
    "homepage": "https://github.com/matsumana/centraldogma-nodejs",
    "repository": {
        "type": "git",
        "url": "git@github.com:matsumana/centraldogma-nodejs.git"
    },
    "engines": {
        "node": ">=12"
    },
    "keywords": [
        "centraldogma"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "fix:prettier:lib": "prettier --write lib",
        "fix:prettier:test": "prettier --write test",
        "fix:eslint:lib": "eslint --ext .js,.ts --fix lib",
        "fix:eslint:test": "eslint --ext .js,.ts --fix test",
        "fix": "run-s fix:prettier:lib fix:prettier:test fix:eslint:lib fix:eslint:test",
        "lint:prettier:lib": "prettier --check lib",
        "lint:prettier:test": "prettier --check test",
        "lint:eslint:lib": "eslint --ext .js,.ts lib",
        "lint:eslint:test": "eslint --ext .js,.ts test",
        "lint": "run-p lint:prettier:lib lint:prettier:test lint:eslint:lib lint:eslint:test",
        "test": "jest",
        "clean": "rm -rf dist",
        "build": "tsc"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/node": "^15.6.1",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.27.0",
        "jest": "^27.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.0",
        "ts-jest": "^27.0.1",
        "typescript": "^4.3.2"
    }
}
